<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mol.quartz.mapper.ExpertUserMapper">
    <select id="getExpertDDIdListByMarId" parameterType="String" resultType="String">
        select dd_id from expert_user where mar_id=#{marId} and authentication='2'
    </select>
</mapper>